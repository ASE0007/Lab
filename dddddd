#include<stdio.h>
#include<stdlib.h>
struct point
{
    int x;
    struct point *next;


};
void create_list(struct point *p)
{

    int n,a;
    printf("enter the number of node :");
    scanf("%d",&n);
    for(int i=1; i<=n; i++)
    {
        scanf("%d",&a);
        p->x = a;
        if(i<n)
        {
            struct point *temp = malloc(sizeof(struct point));
            p->next=temp;
            p=p->next;
        }

        else p->next=NULL;
    }



}
void print_list(struct point *p)
{


    printf("your linked list are :");
    while(p->next !=NULL)
    {
        printf("[%d|*]->",p->x);
        p=p->next;
    }
    printf("[%d|NULL]",p->x);
}

int main(){
struct point *head = malloc(sizeof(struct point));
create_list(head);
print_list(head);
return 0;

}
